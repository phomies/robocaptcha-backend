{
    "root": true,
    "ignorePatterns": [],
    "overrides": [
        {
            "files": ["*.ts"],
            "parserOptions": {
                "project": ["tsconfig.json"],
                "createDefaultProgram": true
            },
            "extends": ["plugin:@typescript-eslint/recommended"],
            "rules": {
                "@typescript-eslint/no-explicit-any": "off",
                "indent": ["error", 4, { "SwitchCase": 1 }]
            }
        }
    ]
}
